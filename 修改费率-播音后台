    def _form_row(self, label_text, input_widget):
        layout = QHBoxLayout()
        label = QLabel(label_text)
        label.setFixedWidth(80)
        layout.addWidget(label)
        layout.addWidget(input_widget)
        return layout

    def load_group_names_from_config(self):
        config = configparser.ConfigParser()
        config.read(CONFIG_FILE)
        if 'GROUP_COOKIES' in config:
            self.group_names = list(config['GROUP_COOKIES'].keys())
        else:
            self.group_names = ['ÁªøËå∂', 'Â¶ñÂß¨', 'Ëâ≤Êàí', 'Áà±Ëâ≤Êí≠', 'ÊÄßÁ¶è', 'Â∞ëÂ•≥', '‰∫∫Â¶ª', '‰πêÂí™']  # ÈªòËÆ§ÂàùÂßãÂÄº

        # ÂàõÂª∫ÂØπÂ∫îËæìÂÖ•Ê°Ü
        for group in self.group_names:
            self.group_inputs[group] = QLineEdit()
            self.group_inputs[group].setPlaceholderText(f"{group} ÁöÑ Cookie")

    def load_config(self):
        config = configparser.ConfigParser()
        config.read(CONFIG_FILE)
        if 'BOT' in config:
            self.token_input.setText(config['BOT'].get('token', ''))
            self.chat_id_input.setText(config['BOT'].get('chat_id', ''))
            self.port_input.setText(config['BOT'].get('port', ''))
            self.code_input.setText(config['BOT'].get('code', ''))
        if 'GROUP_COOKIES' in config:
            for group, line_edit in self.group_inputs.items():
                cookie = config['GROUP_COOKIES'].get(group, '')
                line_edit.setText(cookie)

    def save_config(self):
        config = configparser.ConfigParser()
        config['BOT'] = {
            'token': self.token_input.text(),
            'chat_id': self.chat_id_input.text(),
            'port': self.port_input.text(),
            'code': self.code_input.text()
        }
        config['GROUP_COOKIES'] = {
            group: self.group_inputs[group].text().strip()
            for group in self.group_inputs.keys()
        }
        with open(CONFIG_FILE, 'w') as f:
            config.write(f)
        QMessageBox.information(self, "‰øùÂ≠òÊàêÂäü", "ÈÖçÁΩÆÂ∑≤‰øùÂ≠òÂà∞ config.ini")

    def get_group_cookies(self):
        return {
            group: self.group_inputs[group].text().strip()
            for group in self.group_names
        }

    def run_bot(self):
        if self.running:
            QMessageBox.warning(self, "Â∑≤ËøêË°å", "Êú∫Âô®‰∫∫Â∑≤Âú®ËøêË°å‰∏≠")
            return
        self.running = True
        threading.Thread(target=self.listen_messages, daemon=True).start()
        self.status_label.setText("ÂΩìÂâçÁä∂ÊÄÅÔºö<font color='green'>Êú∫Âô®‰∫∫ËøêË°å‰∏≠</font> ‚úÖ")
        QMessageBox.information(self, "Êú∫Âô®‰∫∫ÂêØÂä®", "Â∑≤ÂºÄÂßãÁõëÂê¨ Telegram Ê∂àÊÅØ")

        # ‚úÖ ÂêØÂä®ÂêéËá™Âä®ÂèëÈÄÅÊµãËØïÊ∂àÊÅØ
        self.send_message("ü§ñ Êú∫Âô®‰∫∫Â∑≤ÂêØÂä®ÔºåÂáÜÂ§áÊé•Êî∂‰øÆÊîπË¥πÁéáÊåá‰ª§„ÄÇ‚úÖ")

    def stop_bot(self):
        if not self.running:
            QMessageBox.information(self, "Êú™ÂêØÂä®", "Êú∫Âô®‰∫∫ÂΩìÂâçÊú™Âú®ËøêË°å")
            return
        self.running = False
        self.status_label.setText("ÂΩìÂâçÁä∂ÊÄÅÔºö<font color='red'>Êú™ÂêØÂä®Êú∫Âô®‰∫∫</font> ‚õî")
        QMessageBox.information(self, "Â∑≤ÂÅúÊ≠¢", "Êú∫Âô®‰∫∫Â∑≤ÂÅúÊ≠¢ÁõëÂê¨")

        # ‚ùå ÂÅúÊ≠¢ÂêéËá™Âä®ÂèëÈÄÅÊµãËØïÊ∂àÊÅØ
        self.send_message("ü§ñ Êú∫Âô®‰∫∫Â∑≤ÂÅúÊ≠¢ÔºåÊó†Ê≥ïÊé•Êî∂‰øÆÊîπË¥πÁéáÊåá‰ª§„ÄÇ‚ùå")

    def send_message(self, text):
        url = f"https://api.telegram.org/bot{self.token_input.text()}/sendMessage"
        data = {
            'chat_id': self.chat_id_input.text(),
            'text': text,
            'parse_mode': 'HTML'
        }
        requests.post(url, data=data)

    def get_updates(self):
        url = f"https://api.telegram.org/bot{self.token_input.text()}/getUpdates"
        params = {'offset': self.last_update_id + 1, 'timeout': 10}
        try:
            r = requests.get(url, params=params)
            if r.status_code == 200:
                data = r.json()
                for result in data['result']:
                    self.last_update_id = result['update_id']
                    if 'message' in result:
                        msg = result['message'].get('text', '')
                        if str(result['message'].get('chat', {}).get('id')) == self.chat_id_input.text():
                            yield msg.strip()
        except:
            pass

    def listen_messages(self):
        while self.running:
            try:
                for msg in self.get_updates():
                    print(f"üì• Êî∂Âà∞Ôºö,\n{msg}")
                    self.process_message(msg)
            except Exception as e:
                print("ÁõëÂê¨ÈîôËØØÔºö", e)
                time.sleep(5)

    def process_message(self, content):
        try:
            lines = content.strip().splitlines()

            if not lines or not any(re.search(r'Ë¥πÁéá\s*[0-9.]+', l) for l in lines):
                self.send_message(
                    "‚ùì Êó†Ê≥ïËØÜÂà´ÁöÑÊåá‰ª§ÔºåËØ∑ÂèëÈÄÅÔºö[Â∞èÁªÑ] ÂïÜÊà∑Âêç Âíå Ë¥πÁéáÊåá‰ª§Ôºå\n‰æãÂ¶ÇÔºö\n[Â∞ëÂ•≥] Â§™Èò≥Âüé 8007\nË¥πÁéá11.3")
                return

            match = re.search(r'Ë¥πÁéá\s*([0-9.]+)', content)
            if not match:
                self.send_message("‚ö†Ô∏è Ë¥πÁéáÊ†ºÂºèÈîôËØØ")
                return

            ÂéüÂßãË¥πÁéáÊñáÊú¨ = match.group(0).replace(" ", "")  # ÁßªÈô§‰∏≠Èó¥Á©∫Ê†º
            ÁõÆÊ†áË¥πÁéá = f"{float(match.group(1)) * 0.01:.3f}"

            cookies_map = self.get_group_cookies()
            ÊàêÂäüÁªìÊûú = []
            Â§±Ë¥•ÈáçËØïÂàóË°® = []

            for line in lines:
                if re.search(r'Ë¥πÁéá\s*[0-9.]+', line):
                    continue

                match = re.match(r'\[(.*?)\]\s*(.+)', line.strip())
                if not match:
                    Â§±Ë¥•ÈáçËØïÂàóË°®.append(line.strip())
                    continue

                Â∞èÁªÑÂêç, ÂïÜÊà∑Âêç = match.group(1), match.group(2)
                cookie = cookies_map.get(Â∞èÁªÑÂêç)
                if not cookie:
                    Â§±Ë¥•ÈáçËØïÂàóË°®.append(f"[{Â∞èÁªÑÂêç}] {ÂïÜÊà∑Âêç}")
                    continue

                try:
                    headers = {'Cookie': f'token={cookie}'}
                    ÊäìÂèñÊé•Âè£ = f"http://v.boinht.com:{self.port_input.text()}/payment_class/pf_list"
                    data = {
                        "name": "", "pay_type": "", "is_effect": "1",
                        "sort_field": "create_time", "sort_type": "desc",
                        "page": 1, "page_size": 200, "channel_id": "",
                        "login_code": self.code_input.text(),
                    }
                    r = requests.post(ÊäìÂèñÊé•Âè£, data=data, headers=headers, timeout=5)
                    if r.status_code != 200:
                        Â§±Ë¥•ÈáçËØïÂàóË°®.append(f"[{Â∞èÁªÑÂêç}] {ÂïÜÊà∑Âêç}")
                        continue

                    items = r.json().get('result', [])
                    found = False
                    for item in items:
                        if ÂïÜÊà∑Âêç == item.get('firm_name', '').strip():
                            found = True
                            item['rate'] = ÁõÆÊ†áË¥πÁéá
                            item['login_code'] = self.code_input.text()
                            ‰øÆÊîπÊé•Âè£ = f"http://v.boinht.com:{self.port_input.text()}/payment_class/edit"
                            resp = requests.post(‰øÆÊîπÊé•Âè£, data=item, headers=headers, timeout=10)
                            if resp.status_code == 200 and resp.json().get("code") == 0:
                                ÊàêÂäüÁªìÊûú.append(f"[{Â∞èÁªÑÂêç}] <b>{item['firm_name']}</b> Ë¥πÁéáÊîπ‰∏∫ <b>{ÁõÆÊ†áË¥πÁéá}</b>‚úÖ")
                            else:
                                Â§±Ë¥•ÈáçËØïÂàóË°®.append(f"[{Â∞èÁªÑÂêç}] {item['firm_name']}")
                            break
                    if not found:
                        Â§±Ë¥•ÈáçËØïÂàóË°®.append(f"[{Â∞èÁªÑÂêç}] {ÂïÜÊà∑Âêç}")
                except:
                    Â§±Ë¥•ÈáçËØïÂàóË°®.append(f"[{Â∞èÁªÑÂêç}] {ÂïÜÊà∑Âêç}")

            if ÊàêÂäüÁªìÊûú:
                self.send_message('\n'.join(ÊàêÂäüÁªìÊûú))

            if Â§±Ë¥•ÈáçËØïÂàóË°®:
                retry_message = ["‰øÆÊîπÂ§±Ë¥•"]
                retry_message += Â§±Ë¥•ÈáçËØïÂàóË°®
                retry_message.append(f"{ÂéüÂßãË¥πÁéáÊñáÊú¨}")  # Áõ¥Êé•ÈôÑ‰∏äË¥πÁéáÔºåÊó†Á©∫Ê†º
                self.send_message('\n'.join(retry_message))

        except Exception:
            self.send_message("‚ùå ÂÖ®Â±ÄÂºÇÂ∏∏ÔºåËØ∑Ê£ÄÊü•Ê∂àÊÅØÊ†ºÂºèÊàñÁ≥ªÁªüÈÖçÁΩÆ")


if __name__ == '__main__':
    app = QApplication(sys.argv)
    gui = BotConfigGUI()
    gui.show()
    sys.exit(app.exec_())
