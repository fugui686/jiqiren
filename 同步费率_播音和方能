import sys
import time
import re
import urllib
from functools import partial
from datetime import datetime
from PyQt5.QtGui import QPixmap, QIcon
from PyQt5.QtCore import QByteArray
import requests
import configparser
import threading
from PyQt5.QtCore import QTimer
from PyQt5.QtWidgets import (
    QApplication, QWidget, QVBoxLayout, QLabel, QLineEdit,
    QTextEdit, QPushButton, QFormLayout, QMessageBox, QGroupBox
)


CONFIG_FILE = 'config.ini'


class ConfigApp(QWidget):
    def __init__(self):
        super().__init__()
        # è®¾ç½®å…è®¸ä½¿ç”¨çš„æˆªæ­¢æ—¶é—´ (YYYY-MM-DD HH:MM:SS)
        self.expiration_date_str = "2027-06-14 23:59:59"  # è½¯ä»¶è¿‡æœŸæ—¶é—´
        self.expiration_date = datetime.strptime(self.expiration_date_str, "%Y-%m-%d %H:%M:%S")

        # æ£€æŸ¥æ˜¯å¦éœ€è¦æé†’
        remaining_days = self.get_remaining_days()
        if remaining_days > 0 and remaining_days <= 7:
            QMessageBox.warning(self, "è½¯ä»¶å³å°†æ›´æ–°",
                                f"æœ¬è½¯ä»¶å°†åœ¨{remaining_days}å¤©åè‡ªåŠ¨æ›´æ–°ï¼š\nè¯·è”ç³»ã€Šå¯Œè´µã€‹è·å–æœ€æ–°ç‰ˆæœ¬ã€‚")

        # æ£€æŸ¥æ˜¯å¦è¿‡æœŸ
        if self.is_expired():
            QMessageBox.critical(self, "è½¯ä»¶å·²æ›´æ–°", f"ä½ çš„åº”ç”¨ç‰ˆæœ¬è¿‡ä½ï¼š\nè¯·è”ç³»ã€Šå¯Œè´µã€‹è·å–æœ€æ–°ç‰ˆæœ¬ã€‚")
            sys.exit(0)  # é€€å‡ºç¨‹åº

        self.setWindowTitle("å¹³å°é…ç½®ç•Œé¢")

        # åŠ è½½ base64 å›¾æ ‡ï¼ˆç¡®ä¿å­—ç¬¦ä¸²å®Œæ•´ï¼‰
        icon_base64 = ""  # æ›¿æ¢æˆä½ çš„ base64
        icon_data = QByteArray.fromBase64(icon_base64.encode())

        # æ›´å¯é çš„å›¾æ ‡åŠ è½½æ–¹å¼
        pixmap = QPixmap()
        if not pixmap.loadFromData(icon_data, "ICO"):  # æ˜¾å¼æŒ‡å®šæ ¼å¼
            print("âŒ å›¾æ ‡åŠ è½½å¤±è´¥ï¼è¯·æ£€æŸ¥ base64 æ•°æ®æˆ–æ–‡ä»¶æ ¼å¼")
        else:
            self.setWindowIcon(QIcon(pixmap))  # è®¾ç½®çª—å£å’Œä»»åŠ¡æ å›¾æ ‡

        self.resize(700, 600)
        self.initUI()
        self.load_config()
        self.polling_thread = threading.Thread(target=self.poll_and_check_cookie_loop, daemon=True)
        self.polling_thread.start()

    def is_expired(self):
        """æ£€æŸ¥å½“å‰æ—¶é—´æ˜¯å¦è¶…è¿‡æŒ‡å®šçš„æˆªæ­¢æ—¶é—´"""
        current_time = datetime.now()  # è·å–å½“å‰ç³»ç»Ÿæ—¶é—´
        return current_time > self.expiration_date

    def get_remaining_days(self):
        """è®¡ç®—è·ç¦»è¿‡æœŸæ—¶é—´è¿˜æœ‰å¤šå°‘å¤©"""
        current_time = datetime.now()  # è·å–å½“å‰ç³»ç»Ÿæ—¶é—´
        days_left = (self.expiration_date - current_time).days  # è®¡ç®—å‰©ä½™å¤©æ•°
        return days_left  # è¿”å›å‰©ä½™çš„å¤©æ•°

    def toggle_smart_mode(self, checked):
        self.smart_mode_enabled = checked
        if checked:
            QMessageBox.information(self, "æ™ºèƒ½æ¨¡å¼", "âœ… æ™ºèƒ½æ¨¡å¼å·²å¼€å¯ï¼Œå°†åœ¨æ¯å°æ—¶çš„ 5/15/25/35/45/55 åˆ†æ‰§è¡Œã€‚")
            self.smart_timer.start(10000)
        else:
            QMessageBox.information(self, "æ™ºèƒ½æ¨¡å¼", "â›”ï¸ æ™ºèƒ½æ¨¡å¼å·²å…³é—­ã€‚")
            self.smart_timer.stop()

    def check_and_run_smart_mode(self):
        now = time.localtime()
        if now.tm_min % 10 == 5 and now.tm_sec < 10:
            print("â° æ™ºèƒ½æ¨¡å¼è§¦å‘ï¼Œå¼€å§‹è¿è¡Œç¨‹åº...")
            self.run_main_program_threaded()

    def poll_and_check_cookie_loop(self):
        while True:
            try:
                domain = self.fields['æ–¹èƒ½åŸŸå'].text()
                user_agent = self.fields['user-agent'].text()
                current_cookie = self.fields['æ–¹èƒ½_cookie'].toPlainText().strip()

                if not domain or not current_cookie:
                    print("â³ Cookie æˆ–åŸŸåä¸ºç©ºï¼Œè·³è¿‡æœ¬è½®æ£€æµ‹ã€‚")
                    time.sleep(60)
                    continue

                headers = {
                    'user-agent': user_agent,
                    'x-requested-with': 'XMLHttpRequest',
                    'cookie': current_cookie
                }

                url = f"https://{domain}/PassManage/Pass/GetPageListJson?page=1&limit=90"
                # print(f"\nâ± æ­£åœ¨å‘èµ· Cookie æ£€æŸ¥è¯·æ±‚åˆ°ï¼š{url}")
                res = requests.get(url, headers=headers, timeout=10)

                print("ğŸ“¦ å“åº”çŠ¶æ€ç ï¼š", res.status_code)
                set_cookie = res.headers.get("Set-Cookie", "")
                match = re.search(r'UserToken=([^;]+)', set_cookie)
                if match:
                    new_token = match.group(1)
                    new_cookie = f"UserToken={new_token}"

                    # URL è§£ç åæ¯”å¯¹ï¼Œç¡®ä¿å®è´¨å†…å®¹ä¸åŒæ‰æ›´æ–°
                    decoded_old = urllib.parse.unquote(current_cookie.replace("UserToken=", "").strip())
                    decoded_new = urllib.parse.unquote(new_token.strip())

                    if decoded_new != decoded_old:
                        print("ğŸ” æ£€æµ‹åˆ° Cookie æ›´æ–°ï¼Œè‡ªåŠ¨æ›¿æ¢")
                        # print("ğŸ” æ–°çš„ UserTokenï¼š", new_cookie)  # âœ… æ‰“å°æ–° Cookie åˆ°æ§åˆ¶å°
                        QTimer.singleShot(0, partial(self.update_cookie_ui, new_cookie))
                        QTimer.singleShot(0, partial(QMessageBox.information,
                                                     self, "Cookie æ›´æ–°", "ğŸ” æ£€æµ‹åˆ°æ–°çš„ UserTokenï¼Œå·²è‡ªåŠ¨æ›´æ–°ï¼"
                                                     ))
                    else:
                        print("âœ… Cookie å†…å®¹ä¸€è‡´ï¼Œæ— éœ€æ›´æ–°")
                else:
                    print("â„¹ï¸ å“åº”ä¸­æ—  Set-Cookie")

            except Exception as e:
                print("âš ï¸ æ£€æŸ¥å¼‚å¸¸ï¼š", str(e))

            print("ğŸ•’ ç­‰å¾… 5 åˆ†é’Ÿåç»§ç»­...")
            time.sleep(5 * 60)

    def update_cookie_ui(self, cookie_value):
        # print("âœ… [UI] æ›´æ–°æ–¹æ³•è§¦å‘ï¼ŒCookie é•¿åº¦ï¼š", len(cookie_value))
        self.fields['æ–¹èƒ½_cookie'].setPlainText(cookie_value)
        # self.save_config()

    def initUI(self):
        main_layout = QVBoxLayout()
        self.fields = {}
        self.long_text_field = 'æ–¹èƒ½_cookie'

        group1 = QGroupBox("ğŸ“¦ æ’­éŸ³å¹³å°è®¾ç½®")
        layout1 = QFormLayout()
        for name in ['port', 'cookie', 'code']:
            label = QLabel(name)
            widget = QLineEdit()
            layout1.addRow(label, widget)
            self.fields[name] = widget
        group1.setLayout(layout1)

        group2 = QGroupBox("ğŸ¢ æ–¹èƒ½å¹³å°é…ç½®")
        layout2 = QFormLayout()
        for name in ['æ–¹èƒ½è´¦å·', 'æ–¹èƒ½å¯†ç ', 'æ–¹èƒ½éªŒè¯ç ', 'æ–¹èƒ½åŸŸå', 'user-agent', 'æ–¹èƒ½_cookie']:
            label = QLabel(name)
            widget = QTextEdit() if name == self.long_text_field else QLineEdit()

            if name == 'æ–¹èƒ½è´¦å·' or name == 'æ–¹èƒ½å¯†ç ':
                widget.setEchoMode(QLineEdit.Password)  # è®¾ç½®ä¸ºå¯†ç è¾“å…¥æ¡†

            if name == self.long_text_field:
                widget.setFixedHeight(80)

            layout2.addRow(label, widget)
            self.fields[name] = widget
        group2.setLayout(layout2)

        group3 = QGroupBox("ğŸ¤– Telegram é€šçŸ¥")
        layout3 = QFormLayout()
        for name in ['æœºå™¨äººToken', 'ç¾¤ç»„ID']:
            label = QLabel(name)
            widget = QLineEdit()
            layout3.addRow(label, widget)
            self.fields[name] = widget
        group3.setLayout(layout3)

        button_layout = QVBoxLayout()
        save_button = QPushButton("ğŸ’¾ ä¿å­˜é…ç½®")
        save_button.clicked.connect(self.save_config)

        run_button = QPushButton("ğŸš€ è¿è¡Œç¨‹åº")
        run_button.clicked.connect(self.run_main_program_threaded)

        smart_button = QPushButton("ğŸ§  æ™ºèƒ½æ¨¡å¼")
        smart_button.setCheckable(True)
        smart_button.clicked.connect(self.toggle_smart_mode)

        login_button = QPushButton("ğŸ” ç™»å½•å¹¶è·å– Cookie")
        login_button.clicked.connect(self.login_fangneng_and_update_cookie)

        button_layout.addWidget(save_button)
        button_layout.addWidget(run_button)
        button_layout.addWidget(smart_button)
        button_layout.addWidget(login_button)

        self.smart_timer = QTimer()
        self.smart_timer.timeout.connect(self.check_and_run_smart_mode)
        self.smart_mode_enabled = False

        main_layout.addWidget(group1)
        main_layout.addWidget(group2)
        main_layout.addWidget(group3)
        main_layout.addLayout(button_layout)
        self.setLayout(main_layout)

        self.setStyleSheet("""
            QWidget {
                background-color: #f0f2f5;  /* è®¾ç½®æ•´ä¸ªçª—å£çš„èƒŒæ™¯é¢œè‰²ä¸ºæµ…ç°è‰² */
                font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;  /* è®¾ç½®å…¨å±€å­—ä½“ä¸ºå¸¸è§çš„æ— è¡¬çº¿å­—ä½“ç³»åˆ— */
            }

            QLineEdit {
                height: 18px;  /* è¾“å…¥æ¡†é«˜åº¦ä¸º 32 åƒç´  */
                font-size: 14px;  /* å­—ä½“å¤§å°ä¸º 11 åƒç´  */
                padding: 4px 8px;  /* è¾“å…¥æ¡†å†…è¾¹è·ï¼šä¸Šä¸‹ 4pxï¼Œå·¦å³ 8px */
                border: 1.5px solid #cccccc;  /* è¾¹æ¡†ä¸ºæµ…ç°è‰²ï¼Œ1.5 åƒç´ å®çº¿ */
                border-radius: 6px;  /* è¾¹è§’åœ†æ»‘ï¼ŒåŠå¾„ 6 åƒç´  */
                background-color: #ffffff;  /* èƒŒæ™¯ä¸ºç™½è‰² */
            }

            QLineEdit:focus {
                border-color: #4a90e2;  /* è·å–ç„¦ç‚¹æ—¶è¾¹æ¡†å˜ä¸ºäº®è“è‰² */
                background-color: #eaf4ff;  /* èƒŒæ™¯è‰²å˜ä¸ºæ·¡è“è‰² */
            }

            QPushButton {
                height: 36px;  /* æŒ‰é’®é«˜åº¦ä¸º 36 åƒç´  */
                font-size: 15px;  /* å­—ä½“å¤§å°ä¸º 15 åƒç´  */
                color: white;  /* å­—ä½“é¢œè‰²ä¸ºç™½è‰² */
                background-color: #4a90e2;  /* æŒ‰é’®é»˜è®¤èƒŒæ™¯ä¸ºäº®è“è‰² */
                border: none;  /* æ— è¾¹æ¡† */
                border-radius: 8px;  /* æŒ‰é’®è¾¹è§’åœ†æ»‘ï¼ŒåŠå¾„ 8 åƒç´  */
                font-weight: bold;  /* å­—ä½“åŠ ç²— */
                margin-top: 10px;  /* é¡¶éƒ¨å¤–è¾¹è·ä¸º 10 åƒç´  */
            }

            QPushButton:hover {
                background-color: #357ABD;  /* é¼ æ ‡æ‚¬åœæ—¶æŒ‰é’®èƒŒæ™¯å˜ä¸ºæ›´æ·±çš„è“è‰² */
            }

            QPushButton:pressed {
                background-color: #2C5AA0;  /* æŒ‰ä¸‹æ—¶æŒ‰é’®èƒŒæ™¯å˜ä¸ºæ›´æ·±æ›´æš—çš„è“è‰² */
            }
        """)

    def login_fangneng_and_update_cookie(self):
        username = self.fields['æ–¹èƒ½è´¦å·'].text()
        password = self.fields['æ–¹èƒ½å¯†ç '].text()
        googlecode = self.fields['æ–¹èƒ½éªŒè¯ç '].text()
        domain = self.fields['æ–¹èƒ½åŸŸå'].text()
        user_agent = self.fields['user-agent'].text()

        session = requests.Session()
        headers = {
            'user-agent': user_agent,
            'x-requested-with': 'XMLHttpRequest',
        }

        try:
            session.get(f"https://{domain}/Home/Login", headers=headers, timeout=5)
            data = {
                'username': username,
                'password': password,
                'googlecode': googlecode,
            }
            res = session.post(f"https://{domain}/Home/LoginJson", headers=headers, data=data, timeout=10)
            token = session.cookies.get("UserToken")
            if token:
                self.fields['æ–¹èƒ½_cookie'].setPlainText(f"UserToken={token}")
                QMessageBox.information(self, "ç™»å½•æˆåŠŸ", f"âœ… ç™»å½•æˆåŠŸï¼Œå·²è·å– UserTokenã€‚\n{token}")
            else:
                QMessageBox.critical(self, "ç™»å½•å¤±è´¥", f"âŒ ç™»å½•å¤±è´¥ï¼Œå“åº”å†…å®¹ï¼š\n{res.text}")
        except Exception as e:
            QMessageBox.critical(self, "å¼‚å¸¸", f"âš ï¸ ç™»å½•è¯·æ±‚å¼‚å¸¸ï¼š{str(e)}")

    def save_config(self):
        config = configparser.ConfigParser(interpolation=None)
        config['DEFAULT'] = {
            k: v.toPlainText() if isinstance(v, QTextEdit) else v.text()
            for k, v in self.fields.items()
        }
        try:
            with open(CONFIG_FILE, 'w') as configfile:
                config.write(configfile)
            QMessageBox.information(self, "æˆåŠŸ", "é…ç½®å·²ä¿å­˜åˆ° config.ini")
        except Exception as e:
            QMessageBox.critical(self, "é”™è¯¯", f"ä¿å­˜å¤±è´¥: {e}")

    def load_config(self):
        config = configparser.ConfigParser(interpolation=None)
        try:
            config.read(CONFIG_FILE)
            for k, v in self.fields.items():
                if k in config['DEFAULT']:
                    if isinstance(v, QTextEdit):
                        v.setPlainText(config['DEFAULT'][k])
                    else:
                        v.setText(config['DEFAULT'][k])
        except Exception as e:
            QMessageBox.warning(self, "è­¦å‘Š", f"åŠ è½½é…ç½®å¤±è´¥: {e}")

    def run_main_program_threaded(self):
        thread = threading.Thread(target=self.run_main_program)
        thread.start()

    def run_main_program(self):
        config = {k: v.toPlainText() if isinstance(v, QTextEdit) else v.text()
                  for k, v in self.fields.items()}

        port = config['port']
        cookie = config['cookie']
        code = config['code']
        api = config['æ–¹èƒ½åŸŸå']
        user = config['user-agent']
        æ–¹èƒ½_cookie = config['æ–¹èƒ½_cookie']
        æœºå™¨äººToken = config['æœºå™¨äººToken']
        ç¾¤ç»„ID = config['ç¾¤ç»„ID']

        æŠ“å–æ¥å£ = f"http://v.boinht.com:{port}/payment_class/pf_list"
        æŸ¥è¯¢æ¥å£ = f'https://{api}/PassManage/Pass/GetPageListJson'
        æ›´æ–°æ¥å£ = f'https://{api}/PassManage/Pass/EditChannelTypeConf'

        def send_telegram_message(message):
            url = f'https://api.telegram.org/bot{æœºå™¨äººToken}/sendMessage'
            data = {
                'chat_id': ç¾¤ç»„ID,
                'text': message,
                'parse_mode': 'Markdown'
            }
            try:
                requests.post(url, data=data, timeout=5)
            except Exception as e:
                print(f"å‘é€ Telegram æ¶ˆæ¯å¤±è´¥: {e}")

        send_telegram_message("âœ… Telegram æµ‹è¯•æ¶ˆæ¯ï¼šç¨‹åºå·²å¯åŠ¨")

        def get_boinht_headers(token):
            return {'Cookie': f'token={token}'}

        æ›´æ–°_headers = {
            'cookie': æ–¹èƒ½_cookie,
            'user-agent': user,
            'x-requested-with': 'XMLHttpRequest',
        }

        æ‰€æœ‰é€šé“åˆ—è¡¨ = []
        å·²å­˜åœ¨é›†åˆ = set()
        print("ğŸŸ¡ å¼€å§‹æŠ“å–é€šé“åˆ—è¡¨...")

        data = {
            "name": "", "pay_type": "", "is_effect": "1",
            "sort_field": "create_time", "sort_type": "desc",
            "page": 1, "page_size": 200, "channel_id": "",
            "login_code": code,
        }

        try:
            response = requests.post(æŠ“å–æ¥å£, data=data, headers=get_boinht_headers(cookie), verify=False, timeout=5)
            if response.status_code == 200:
                items = response.json().get('result', [])
                for item in items:
                    firm_name = item.get('firm_name', '')
                    rate_raw = item.get('rate', '')
                    if '%' in firm_name:
                        continue
                    match = re.search(r'([\u4e00-\u9fa5]+)\s*(\d+)', firm_name)
                    if match:
                        å•†æˆ·åç§° = match.group(1)
                        å•†æˆ·ç¼–ç  = match.group(2)
                        è´¹ç‡ = rate_raw
                        å”¯ä¸€é”® = (å•†æˆ·åç§°, å•†æˆ·ç¼–ç , è´¹ç‡)
                        if å”¯ä¸€é”® not in å·²å­˜åœ¨é›†åˆ:
                            å·²å­˜åœ¨é›†åˆ.add(å”¯ä¸€é”®)
                            æ‰€æœ‰é€šé“åˆ—è¡¨.append({
                                'å•†æˆ·åç§°': å•†æˆ·åç§°,
                                'å•†æˆ·ç¼–ç ': å•†æˆ·ç¼–ç ,
                                'è´¹ç‡': è´¹ç‡
                            })
            else:
                print(f"è¯·æ±‚å¤±è´¥ï¼ŒçŠ¶æ€ç ï¼š{response.status_code}")
        except Exception as e:
            print(f"è¯·æ±‚å¼‚å¸¸ï¼š{str(e)}")

        æ‰€æœ‰é€šé“åˆ—è¡¨.sort(key=lambda x: (x['å•†æˆ·åç§°'], int(x['å•†æˆ·ç¼–ç '])))
        print(f"âœ… æŠ“å–å®Œæˆï¼Œå…±æ”¶é›†é€šé“æ•°ï¼š{len(æ‰€æœ‰é€šé“åˆ—è¡¨)}")

        print("ğŸŸ¡ å¼€å§‹é€æ¡æ›´æ–°è´¹ç‡ï¼ˆé—´éš”1ç§’ï¼‰...")
        æˆåŠŸæ•°é‡ = 0
        è·³è¿‡æ•°é‡ = 0
        æˆåŠŸåˆ—è¡¨ = []

        for item in æ‰€æœ‰é€šé“åˆ—è¡¨:
            å•†æˆ·åç§° = item['å•†æˆ·åç§°']
            å•†æˆ·ç¼–ç  = str(item['å•†æˆ·ç¼–ç '])
            ç›®æ ‡è´¹ç‡ = item['è´¹ç‡']

            æŸ¥è¯¢å‚æ•° = {
                'page': '1', 'limit': '90', 'name': å•†æˆ·åç§°,
                'tGGroupId': '', 'state': '1',
            }

            try:
                r = requests.get(æŸ¥è¯¢æ¥å£, params=æŸ¥è¯¢å‚æ•°, headers=æ›´æ–°_headers, timeout=10)
                if r.status_code == 200:
                    data = r.json().get('PageData', [])
                    if not data:
                        print(f"âŒ å•†æˆ· {å•†æˆ·åç§°} æ— æ•°æ®")
                        continue
                    for aa in data:
                        if str(aa.get('ChannelType')) == å•†æˆ·ç¼–ç :
                            å½“å‰è´¹ç‡ = str(aa.get('PassChannelTypeConfRate')).strip()
                            try:
                                å½“å‰è´¹ç‡_f = round(float(å½“å‰è´¹ç‡), 4)
                                ç›®æ ‡è´¹ç‡_f = round(float(ç›®æ ‡è´¹ç‡), 4)
                            except Exception as e:
                                print(f"âš ï¸ è´¹ç‡æ ¼å¼å¼‚å¸¸ï¼Œè·³è¿‡é€šé“ {å•†æˆ·åç§°} - {å•†æˆ·ç¼–ç }ï¼š{e}")
                                break

                            # ğŸ‘‡ æ‰“å°æ¯”å¯¹ç»“æœ
                            if å½“å‰è´¹ç‡_f == ç›®æ ‡è´¹ç‡_f:
                                print(f"â­ å•†æˆ· {å•†æˆ·åç§°} | é€šé“ {å•†æˆ·ç¼–ç } è´¹ç‡ä¸€è‡´ï¼ˆ{å½“å‰è´¹ç‡_f}ï¼‰ï¼Œè·³è¿‡")
                                è·³è¿‡æ•°é‡ += 1
                                break
                            é€šé“ID = aa.get('PassChannelTypeConfId')
                            å¤‡æ³¨ = aa.get('PassCodeName')
                            é‡‘é¢èŒƒå›´ = aa.get('AmountRange')
                            é€šé“ç¼–ç  = aa.get('ChannelType')

                            payload = {
                                'Id': é€šé“ID, 'Status': '1',
                                'PassId': '', 'remark': å¤‡æ³¨,
                                'Rate': ç›®æ ‡è´¹ç‡, 'AmountRange': é‡‘é¢èŒƒå›´,
                                'ChannelType': é€šé“ç¼–ç , 'AgentRate': '0',
                            }

                            res = requests.post(æ›´æ–°æ¥å£, data=payload, headers=æ›´æ–°_headers, timeout=10)
                            print(f"âœ… å•†æˆ· {å•†æˆ·åç§°} | é€šé“ {å•†æˆ·ç¼–ç } æ›´æ–°æˆåŠŸ çŠ¶æ€ç : {res.status_code}")
                            æˆåŠŸæ•°é‡ += 1
                            æˆåŠŸåˆ—è¡¨.append(f"{å•†æˆ·åç§°}({å•†æˆ·ç¼–ç }) æ›´æ–°åè´¹ç‡ {ç›®æ ‡è´¹ç‡}")
                            time.sleep(1)
                            break
                else:
                    print(f"âŒ æŸ¥è¯¢å¤±è´¥ - å•†æˆ· {å•†æˆ·åç§°}ï¼ŒçŠ¶æ€ç : {r.status_code}")
            except Exception as e:
                print(f"å¼‚å¸¸ - å•†æˆ· {å•†æˆ·åç§°}: {str(e)}")

        æ€»æ•° = æˆåŠŸæ•°é‡ + è·³è¿‡æ•°é‡
        æˆåŠŸé€šé“æ–‡æœ¬ = '\n'.join(æˆåŠŸåˆ—è¡¨)
        if not æˆåŠŸé€šé“æ–‡æœ¬:
            æˆåŠŸé€šé“æ–‡æœ¬ = "æ— "

        message = (
            f"ğŸ“¢ å¹³å°è´¹ç‡æ›´æ–°å®Œæˆ\n"
            f"âœ… æˆåŠŸæ›´æ–°é€šé“æ•°ï¼š{æˆåŠŸæ•°é‡}\n"
            f"â­ è·³è¿‡è´¹ç‡ä¸€è‡´é€šé“æ•°ï¼š{è·³è¿‡æ•°é‡}\n"
            f"ğŸ“¦ æ€»é€šé“æ•°ï¼š{æ€»æ•°}\n"
            f"ğŸ§¾ å·²æ›´æ–°é€šé“ï¼š\n{æˆåŠŸé€šé“æ–‡æœ¬}\n"
            f"â° æ—¶é—´ï¼š{time.strftime('%Y-%m-%d %H:%M:%S')}"
        )
        send_telegram_message(message)
        print("ğŸ‰ æ‰€æœ‰å¤„ç†å®Œæˆã€‚")

        def show_finish_message():
            QMessageBox.information(
                self, "å®Œæˆ",
                f"âœ… æˆåŠŸæ›´æ–°ï¼š{æˆåŠŸæ•°é‡} æ¡\nâ­ è·³è¿‡ï¼š{è·³è¿‡æ•°é‡} æ¡\nğŸ“¦ æ€»å…±ï¼š{æ€»æ•°} æ¡"
            )

        QTimer.singleShot(0, show_finish_message)


if __name__ == '__main__':
    app = QApplication(sys.argv)
    window = ConfigApp()
